    "highlight.js": "^9",
    "markdown-it": "^8"
  },
  "peerDependencies": {
    "nuxt": "^1"
  },
  "readmeFilename": "./README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/nuxt-blog.git"
  },
  "scripts": {
    "fixpack": "fixpack",
    "nuxt": "nuxt",
    "precommit": "lint-staged",
    "test": "eslint . && ava"
  }
